{
    "name": "soul-rise/arrayaccess-container",
    "description": "ArrayAccess adapter for laminas/laminas-servicemanager",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "laminas",
        "mezzio",
        "psr",
        "di",
        "dic",
        "dependency-injection",
        "psr-11",
        "servicemanager",
        "service-manager",
        "phpmig",
        "adapter",
        "arrayaccess",
        "arrayaccess-adapter"
    ],
    "authors": [
        {
            "name": "Dmitriy Krasnorutskiy",
            "email": "soul-rise@ya.ru"
        }
    ],
    "support": {
        "docs": "https://github.com/soul-rise/",
        "issues": "https://github.com/soul-rise/servicemanager-array-access/issues",
        "source": "https://github.com/soul-rise/servicemanager-array-access"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "laminas/laminas-servicemanager": "^3.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SoulRise\\ArrayAccess\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
